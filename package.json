{
  "name": "kubernetes-canary-action",
  "version": "1.2.6",
  "private": true,
  "description": "An action to handle determining valid kubernetes rollbacks",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "version": "npm run build && npm run package && git add -- dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartlyio/kubernetes-rollback-action.git"
  },
  "keywords": [
    "actions",
    "kubernetes",
    "deployment",
    "lifecycle",
    "rollback"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1"
  },
  "devDependencies": {
    "@types/jest": "29.2.4",
    "@types/node": "16.18.8",
    "@typescript-eslint/parser": "5.46.0",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.29.0",
    "eslint-plugin-github": "4.6.0",
    "eslint-plugin-jest": "27.1.6",
    "jest": "29.3.1",
    "jest-circus": "29.3.1",
    "js-yaml": "4.1.0",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  }
}
